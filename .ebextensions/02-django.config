  container_commands:
    01_migrate:
      command: "source $PYTHONPATH/activate && python manage.py migrate" 
      leader_only: true
    02_compilemessages:
      command: "source $PYTHONPATH/activate && python manage.py compilemessages"
    03_createsu:
      command: "source $PYTHONPATH/activate && python manage.py createsu"
    04_collectstatic:
      command: "source $PYTHONPATH/activate && python manage.py collectstatic --noinput"
    05_seedusers:
      command: "source $PYTHONPATH/activate && python manage.py seed_users --number 50"
    06_seedamenities:
      command: "source $PYTHONPATH/activate && python manage.py seed_amenities"
    07_seedfacilities:
      command: "source $PYTHONPATH/activate && python manage.py seed_facilities"
    08_seedrooms:
      command: "source $PYTHONPATH/activate && python manage.py seed_rooms --number 40"
    09_seedreservations:
      command: "source $PYTHONPATH/activate && python manage.py seed_reservations --number 20"
    10_seedreviews:
      command: "source $PYTHONPATH/activate && python manage.py seed_reviews --number 20"
    

    
  option_settings:
    aws:elasticbeanstalk:container:python:
      WSGIPath: config.wsgi:application
    aws:elasticbeanstalk:application:environment:
      DJANGO_SETTINGS_MODULE: config.settings