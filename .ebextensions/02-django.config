  container_commands:
    01_migrate:
      command: "source /var/app/venv/*/bin/activate && cd /$PYTHONPATH && python manage.py migrate" 
      leader_only: true
    02_compilemessages:
      command: "source /var/app/venv/*/bin/activate && cd /$PYTHONPATH && python manage.py compilemessages"
    03_createsu:
      command:  "source /var/app/venv/*/bin/activate && cd /$PYTHONPATH && python manage.py createsu"
    04_collectstatic:
      command: "source /var/app/venv/*/bin/activate && cd /$PYTHONPATH && python manage.py collectstatic --noinput"
  option_settings:
    aws:elasticbeanstalk:container:python:
      WSGIPath: config.wsgi:application
    aws:elasticbeanstalk:application:environment:
      DJANGO_SETTINGS_MODULE: config.settings